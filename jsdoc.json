{
	"source": {
		"include": ["./"],
		"includePattern": ".js$",
		"exclude": [
			"node_modules",
			"./static/pixi-legacy.js",
			"./static/pixi.js"
		],
		"excludePattern": "(node_module/|docs)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/app-jsdoc"
	}
}
