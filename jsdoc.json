{
	"source": {
		"include": ["./"],
		"includePattern": ".js$",
		"exclude": ["node_modules"],
		"excludePattern": "(node_module/|docs)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/app-jsdoc"
	}
}
