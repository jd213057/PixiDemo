{
	"trailingComma": "es5",
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "consistent",
	"printWidth": 80,
	"jsxSingleQuote": true,
	"jsxBracketSameLine": true,
	"bracketSpacing": false,
	"arrowParens": "always",
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": true,
	"embeddedLanguageFormatting": "auto"
}
